<div class="row">
  <div class="col-md-6 offset-md-3">
    <h1>
      <%= @user.username %>
    </h1>

    <ul class="nav nav-pills nav-justified">

      <li class="nav-item">
        <%= link_to "Posts", user_path(@user.username), class: "nav-link" %>
      </li>

      <li class="nav-item">
        <%= link_to "Liked photos", liked_photos_path(@user.username), class: "nav-link" %>
      </li>

      <li class="nav-item">
        <%= link_to "Feed", users_feed_path(@user.username), class: "nav-link" %>
      </li>

      <li class="nav-item">
        <%= link_to "Followers", users_followers_path(@user.username), class: "nav-link" %>
      </li>

      <li class="nav-item">
        <%= link_to "Following", users_following_path(@user.username), class: "nav-link" %>
      </li>
      
    </ul>
  </div>
</div>

<%= current_user.feed.count %>

<% current_user.feed.each do |zebra| %>
  <%= render "photos/photo", giraffe: zebra %>
<% end %>
